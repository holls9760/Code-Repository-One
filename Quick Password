attemptcount=0
while attemptcount<5:
    password="changeme"
    question=input ("enter password")
    attemptcount+=1
    print ("number of attempts left", 5-attemptcount)
    if question!='changeme':
        print ("incorrect password")
    if question=='changeme':
        print ("yay. you did it in", attemptcount, "number of attempts")
        break

if attemptcount >=5:
    print ("access denied")
    
    
