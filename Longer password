password="changeme"
question=input("enter password")
if question!='changeme':
    print ("incorrect password. please enter again")
    question=input("enter password")
    if question!='changeme':
        print ("incorrect password. please enter again")
        question=input("enter password")
        if question!='changeme':
            print ("incorrect password. please enter again")
            question=input("enter password")
            if question!='changeme':
                print ("incorrect password. please enter again")
                question=input("enter password")
                if question!='changeme':
                    print ("incorrect password. you've been locked out")
if password=='changeme':
    print ("you're in!") 
